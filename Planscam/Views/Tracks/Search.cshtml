@model Planscam.Models.TrackSearchViewModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}


@using(Html.BeginForm(FormMethod.Get))
{
    @Html.EditorForModel()
    <input type="submit" value="Search"/>
}

@if (Model is { Result: { } })
{
    @foreach (var track in Model.Result.Tracks!)
    {
        @await Html.PartialAsync("TrackInList", new TrackViewModel
        {Picture = track.Picture, Name = track.Name, Id = track.Id, IsLiked = track.IsLiked, PlaylistId = 0})
    }
}