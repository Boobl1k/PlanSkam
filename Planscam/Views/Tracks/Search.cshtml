@model Planscam.Models.TrackSearchViewModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}


@using(Html.BeginForm(FormMethod.Post))
{
    @Html.EditorForModel()
    <input type="submit" value="Search"/>
}

@if (Model is { Result: { } })
{
    @await Html.PartialAsync("PlaylistFull", Model.Result)
}